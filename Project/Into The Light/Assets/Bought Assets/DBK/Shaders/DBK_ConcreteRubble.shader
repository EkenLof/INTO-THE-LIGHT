				float PaintSelection123 = ( saturate( ( 1.0 - ( ( distance( temp_cast_4 , temp_cast_5 ) - _PaintRange ) / max( _PaintSmooth , 1E-05 ) ) ) ) * ( 1.0 - MaskABlue76 ) );
