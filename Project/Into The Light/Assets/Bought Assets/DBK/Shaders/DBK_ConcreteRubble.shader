				float3 Normals71 = lerpResult10;
				
				float2 ConcreteUVTiling73 = texCoord18;
				float4 tex2DNode85 = tex2D( _MaskB, ConcreteUVTiling73 );
				float Mask_B_Alpha89 = tex2DNode85.a;
				float MaskARed84 = tex2DNode75.r;
				float VertexRed125 = packedInput.ase_color.r;
				float3 temp_cast_2 = (( VertexRed125 * Mask_B_Alpha89 )).xxx;
				float Mask_B_Red86 = tex2DNode85.r;
				float3 temp_cast_3 = (( ( 1.0 - Mask_B_Red86 ) + ( ( 1.0 - MaskARed84 ) * _DirtInsideMultiplier ) )).xxx;
				float clampResult63 = clamp( ( saturate( ( 1.0 - ( ( distance( temp_cast_2 , temp_cast_3 ) - _DirtRange ) / max( _DirtSmooth , 1E-05 ) ) ) ) * _DirtContrast ) , 0.0 , 1.0 );
				float DirtSelection136 = clampResult63;
				float3 temp_cast_4 = (( 1.0 - Mask_B_Red86 )).xxx;
