				float2 temp_cast_1 = (_DamageNMTiling).xx;
				float2 texCoord11 = packedInput.ase_texcoord3.xy * temp_cast_1 + float2( 0,0 );
				float3 unpack3 = UnpackNormalScale( tex2D( _DamageNM, texCoord11 ), _DamageScale );
				unpack3.z = lerp( 1, unpack3.z, saturate(_DamageScale) );
